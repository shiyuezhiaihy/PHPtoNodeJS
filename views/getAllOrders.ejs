<!DOCTYPE html>
<html>
<head>
    <title>TEST</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
<h1>ORDERS</h1>
<div class="container">
    <h2>Database Results getting Orders </h2>
    <ul>
        <% results.forEach(function(r) { %>
        <li><%= r.CUSTOMER_ID %></li>
        <li><%= r.SHIPPING_ID %></li>
        <li><%= r.ORDER_TOTAL %></li>
        <ul>
            <li>From:( <%=r.START_Longitude %>, <%=r.START_Latitude %> )</li>
            <li>TO: ( <%=r.END_Longitude %>,<%=r.END_Latitude %> ) </li>
        </ul>
        <br/><br/>
        <% }); %>
    </ul>
</div>

</body>
</html>